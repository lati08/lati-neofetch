print_info() {

    info title
    prin "$(color 3)╭─────────────"
    info "$(color 3)├─DIS" distro
    info "$(color 3)├─PKG" packages
    info "$(color 3)├─CPU" cpu
    info "$(color 3)├─GPU" gpu
    info "$(color 3)╰─MMR" memory
}
separator=" ❖"
os_arch="on"
distro_shorthand="off"
ascii_distro="Arch_small"
colors=(14 8 8 8 8 8 8 8 8 8 8 8 8 )
ascii_colors=(14)
